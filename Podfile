  pod 'MCDateExtensions', :git => 'https://github.com/mirego/MCDateExtensions.git'
  pod 'MZFormSheetPresentationController', '~> 2.4'
  pod 'SSKeychain'
  pod 'SVProgressHUD'
  pod 'SwiftyJSON', '~> 2.3'
  pod 'TTTAttributedLabel'
end
target 'Easy Digital Downloads' do
  pod 'Alamofire', '~> 3.4'
  pod 'AlamofireNetworkActivityIndicator', '~> 1.0'
  pod 'BEMSimpleLineGraph'
  pod 'CocoaLumberjack/Swift'
